nse\r\n            let data = null\r\n\r\n            switch (method) {\r\n                case 'GET':\r\n                    response = await axios.get(url)\r\n                    break\r\n                case 'POST':\r\n                    response = await axios.post(url, { method, body, headers })\r\n                    break\r\n                default:\r\n                    throw new Error('Что-то пошло не так')\r\n            }\r\n\r\n            if (!response.data.success) {\r\n                alert(response.data.message);\r\n            } else {\r\n                data = response.data.message\r\n            }\r\n\r\n            return data\r\n        } catch (e) {\r\n            alert('Что-то пошло не так');\r\n        }\r\n    }, [])\r\n\r\n    return request\r\n}","C:\\Users\\Zabilsya\\Desktop\\Cursovaya\\Calendar\\client\\src\\URLs.js",[],"C:\\Users\\Zabilsya\\Desktop\\Cursovaya\\Calendar\\client\\src\\context\\ScheduleContext.js",[],"C:\\Users\\Zabilsya\\Desktop\\Cursovaya\\Calendar\\client\\src\\hooks\\socket.hook.js",["116","117"],"import {useState, useCallback, useEffect} from 'react'\r\nimport io from 'socket.io-client';\r\nimport URLs from './../URLs';\r\n\r\nexport const useSocket = () => {\r\n    const [events, setEvents] = useState(null)\r\n\r\n    const socket = io(`${URLs.socketURL}/socket`);\r\n\r\n    socket.on(\"newEvent\", (event) => {\r\n      setEvents([...events, event])\r\n    });\r\n\r\n    socket.on(\"deletedEvent\", (id) => {\r\n      const updatedThoughts = this.state.thoughts.filter((thought) => {\r\n        return thought._id !== id;\r\n      });\r\n\r\n      this.setState({ thoughts: updatedThoughts });\r\n    });\r\n\r\n\r\n}","C:\\Users\\Zabilsya\\Desktop\\Cursovaya\\Calendar\\client\\src\\Components\\Modal\\Modal.js",["118"],"import React, { useEffect, useState } from 'react'\r\nimport ReactDOM from 'react-dom';\r\nimport { Event } from './../Event/Event'\r\n\r\nimport './css/style.css'\r\n\r\nexport const Modal = ({ title, on